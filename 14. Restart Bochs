. lib/functions
basename "$0"
export noks=0

ip=$(ifconfig eth0|grep "inet addr:"|awk '{print $2}'|sed -e's/.*://')
ifconfig eth0
ifconfig eth0|grep "inet addr:"|awk '{print $2}'|sed -e's/.*://'
sed -i -e "s/10.0.0.1/$ip/" _2
echo $ip
cat _2

>out
(until grep -q '#' out ; do sleep 5; done ; sleep 5; slowcat ./_2 2 1)| TERM=vt100 bochs -q -f bochsrc |tee -a out 

echo .
cat out
echo .
od -cv out

exit $noks
