. lib/functions
basename "$0"
export noks=0

touch out
echo $TERM
(until grep -q '#' out ; do sleep 5; done ; slowcat ./_1 2 .1)| TERM=vt100 bochs -q -f bochsrc |tee -a out >/dev/null 2>&1

cat out
fdisk -l disk.img
echo .
(echo b; echo p)|fdisk disk.img
dd if=disk.img bs=512 count=1|od -v -tx1
exit $noks
